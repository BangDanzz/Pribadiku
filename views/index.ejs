<script>
document.addEventListener("DOMContentLoaded", () => {
  const baseUrl = "https://api.hitori.biz.id"; // ubah jika kamu pakai domain/API sendiri
  const modalEl = document.getElementById("apiModal");
  const modal = new bootstrap.Modal(modalEl);

  const apiUrlField = document.getElementById("apiUrl");
  const copyUrlBtn = document.getElementById("copyUrlBtn");
  const fileUploadSection = document.getElementById("fileUploadSection");
  const fileInput = document.getElementById("fileInput");
  const uploadBtn = document.getElementById("uploadBtn");
  const modalLoading = document.getElementById("modalLoading");
  const modalPreviewWrapper = document.getElementById("modalPreviewWrapper");
  const modalPreviewImg = document.getElementById("modalPreviewImg");
  const modalPreviewVideo = document.getElementById("modalPreviewVideo");
  const modalDownload = document.getElementById("modalDownload");
  const modalContent = document.getElementById("modalContent");

  copyUrlBtn.addEventListener("click", () => {
    apiUrlField.select();
    document.execCommand("copy");
  });

  function resetModal() {
    fileUploadSection.style.display = "none";
    modalLoading.style.display = "none";
    modalPreviewWrapper.style.display = "none";
    modalPreviewImg.style.display = "none";
    modalPreviewVideo.style.display = "none";
    modalContent.style.display = "none";
    modalPreviewImg.src = "";
    modalPreviewVideo.src = "";
    modalDownload.href = "#";
    modalContent.textContent = "";
    apiUrlField.value = "";
  }

  function showLoading(endpoint, text = "Loading...") {
    resetModal();
    apiUrlField.value = endpoint;
    modalLoading.querySelector("p").textContent = text;
    modalLoading.style.display = "block";
    modal.show();
  }

  function showPreview(endpoint, url, type = "image") {
    resetModal();
    apiUrlField.value = endpoint;
    modalPreviewWrapper.style.display = "block";

    if (type === "video") {
      modalPreviewVideo.style.display = "block";
      modalPreviewVideo.src = url;
    } else {
      modalPreviewImg.style.display = "block";
      modalPreviewImg.src = url;
    }

    modalDownload.href = url;
    modalDownload.setAttribute("download", `hitori-${Date.now()}.${type === "video" ? "mp4" : "png"}`);
    modal.show();
  }

  function showJson(endpoint, jsonData) {
    resetModal();
    apiUrlField.value = endpoint;
    modalContent.style.display = "block";
    modalContent.textContent = JSON.stringify(jsonData, null, 2);
    modal.show();
  }

  function showText(endpoint, text) {
    resetModal();
    apiUrlField.value = endpoint;
    modalContent.style.display = "block";
    modalContent.textContent = text;
    modal.show();
  }

  document.querySelectorAll(".api-started-btn, .api-preview-btn").forEach(btn => {
    btn.addEventListener("click", async () => {
      document.querySelectorAll(".api-started-btn").forEach(b => b.classList.remove("active"));
      btn.classList.add("active");

      let endpoint = btn.getAttribute("data-endpoint");
      if (!endpoint) return;
      if (endpoint.startsWith("/")) endpoint = baseUrl + endpoint;

      const type = btn.getAttribute("data-type"); // upload atau preview
      resetModal();

      if (type === "upload") {
        fileUploadSection.style.display = "block";
        modal.show();
      } else {
        showLoading(endpoint);
        try {
          const res = await fetch(endpoint);
          const ct = res.headers.get("content-type") || "";

          if (ct.includes("application/json")) {
            const json = await res.json();
            if (json?.url && typeof json.url === "string") {
              const ext = json.url.split(".").pop().toLowerCase();
              const mediaType = ["mp4", "webm"].includes(ext) ? "video" : "image";
              if (mediaType === "image") {
                try {
                  const imgRes = await fetch(json.url);
                  const blob = await imgRes.blob();
                  const blobUrl = URL.createObjectURL(blob);
                  showPreview(endpoint, blobUrl, "image");
                } catch (e) {
                  showText(endpoint, "JSON contains image URL but cannot fetch: " + e.message);
                }
              } else {
                showPreview(endpoint, json.url, "video");
              }
            } else {
              showJson(endpoint, json);
            }
          } else if (ct.startsWith("image/")) {
            const blob = await res.blob();
            const url = URL.createObjectURL(blob);
            showPreview(endpoint, url, "image");
          } else if (ct.startsWith("video/")) {
            const blob = await res.blob();
            const url = URL.createObjectURL(blob);
            showPreview(endpoint, url, "video");
          } else {
            const text = await res.text();
            showText(endpoint, text);
          }
        } catch (err) {
          showText(endpoint, "Error: " + err.message);
        }
      }
    });
  });

  uploadBtn.addEventListener("click", async () => {
    const file = fileInput.files[0];
    if (!file) return alert("Please select a file");

    const activeBtn = document.querySelector(".api-started-btn.active");
    if (!activeBtn) return showText("", "Error: tombol upload tidak aktif");

    const endpoint = activeBtn.getAttribute("data-endpoint");
    const formData = new FormData();
    formData.append("file", file);

    showLoading(endpoint, "Uploading...");

    try {
      const res = await fetch(endpoint, { method: "POST", body: formData });
      const data = await res.json();

      if (data.status && data.url) {
        const ext = data.url.split(".").pop().toLowerCase();
        const type = ["mp4", "webm"].includes(ext) ? "video" : "image";
        showPreview(endpoint, data.url, type);
      } else {
        showJson(endpoint, data);
      }
    } catch (err) {
      showText(endpoint, "Error: " + err.message);
    }
  });

  modalEl.querySelectorAll('[data-bs-dismiss="modal"]').forEach(btn => {
    btn.addEventListener("click", () => modal.hide());
  });
});
</script>

