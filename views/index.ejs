  .thanks-list li:hover {
    color: #28a745;
    font-weight: 600;
    transform: translateX(5px);
  }

  .thanks-list li:hover::after {
    content: "‚≠ê";
    position: absolute;
    right: 0;
    top: 0;
    font-size: 14px;
    color: #ffc107;
    opacity: 0;
    transform: translateY(5px);
    animation: fadeUp 0.3s forwards;
  }

