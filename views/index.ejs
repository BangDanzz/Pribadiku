<script>
document.addEventListener("DOMContentLoaded", () => {
  const buttons = document.querySelectorAll(".api-started-btn");
  const modal = new bootstrap.Modal(document.getElementById("apiModal"));
  const modalContent = document.getElementById("modalContent");
  const apiUrlField = document.getElementById("apiUrl");
  const copyBtn = document.getElementById("copyUrlBtn");
  const copyToastEl = document.getElementById('copyToast');
  const copyToast = new bootstrap.Toast(copyToastEl);

  buttons.forEach(btn => {
    btn.addEventListener("click", async () => {
      const endpoint = btn.dataset.endpoint;
      const fullUrl = window.location.origin + endpoint;
      apiUrlField.value = fullUrl;
      modalContent.textContent = "Loading...";
      modal.show();

      try {
        const res = await fetch(fullUrl);
        const data = await res.json();
        modalContent.textContent = JSON.stringify(data, null, 2);
      } catch (err) {
        modalContent.textContent = "Error: " + err.message;
      }
    });
  });

  copyBtn.addEventListener("click", () => {
    apiUrlField.select();
    apiUrlField.setSelectionRange(0, 99999);
    navigator.clipboard.writeText(apiUrlField.value)
      .then(() => copyToast.show())
      .catch(err => alert("Gagal menyalin URL: " + err));
  });
});
</script>

<!-- Script Search + Highlight -->
<script>
  function highlight(text, search) {
    if (!search) return text;
    let regex = new RegExp(`(${search})`, "gi");
    return text.replace(regex, '<span class="highlight">$1</span>');
  }

  document.getElementById('searchInput').addEventListener('keyup', function () {
    let filter = this.value.toLowerCase();
    let rows = document.querySelectorAll("#featuresTable tbody tr");

    rows.forEach(row => {
      let match = false;
      row.querySelectorAll("td").forEach(td => {
        let original = td.textContent;
        if (original.toLowerCase().includes(filter) && filter) {
          match = true;
          td.innerHTML = highlight(original, filter);
        } else {
          td.innerHTML = original; // reset highlight
        }
      });
      row.style.display = (match || !filter) ? "" : "none";
    });
  });
</script>
