<script>
document.addEventListener('DOMContentLoaded', function () {
  const selectElement = document.getElementById('actionSelect');

  function toggleSections() {
    const sections = [
      'addprem', 'addvip', 'delprem', 'deluser',
      'listprem', 'redeemprem', 'redeempremdel', 'banip'
    ];
    sections.forEach(id => {
      const el = document.getElementById(id);
      if (el) el.style.display = 'none';
    });

    if (selectElement.value === 'addVip') 
      document.getElementById('addvip').style.display = 'block';
    else if (selectElement.value === 'addPrem') 
      document.getElementById('addprem').style.display = 'block';
    else if (selectElement.value === 'delPrem') 
      document.getElementById('delprem').style.display = 'block';
    else if (selectElement.value === 'delUser') 
      document.getElementById('deluser').style.display = 'block';
    else if (selectElement.value === 'listPrem') 
      document.getElementById('listprem').style.display = 'block';
    else if (selectElement.value === 'redeemPrem') 
      document.getElementById('redeemprem').style.display = 'block';
    else if (selectElement.value === 'redeemPremDel') 
      document.getElementById('redeempremdel').style.display = 'block';
    else if (selectElement.value === 'banIp') 
      document.getElementById('banip').style.display = 'block';
  }

  function timer(milliseconds) {
    const days = Math.floor(milliseconds / (24*60*60*1000));
    const hours = Math.floor((milliseconds % (24*60*60*1000)) / (60*60*1000));
    const minutes = Math.floor((milliseconds % (60*60*1000)) / (60*1000));
    return `${days} Days ${hours} Hours ${minutes} Minutes`;
  }

  function fetchPremiumUsers() {
    $.ajax({
      url: '/admin/premium/list',
      method: 'GET',
      dataType: 'json',
      success: function(data) {
        const premiumList = document.getElementById('premiumList');
        premiumList.innerHTML = '';
        data.forEach(user => {
          const prof = user.profile;
          const role = user.role;
          const userItem = document.createElement('div');
          userItem.innerHTML = `
            <a class="d-flex align-items-center border-bottom py-3">
              <div class="me-3"><img src="${prof}" class="rounded-circle wd-35" alt="user"></div>
              <div class="w-100">
                <div class="d-flex justify-content-between">
                  <h6 class="text-body mb-2">${user.name}</h6>
                  <p class="text-muted tx-12">${role}</p>
                </div>
                <p class="text-muted tx-13">${timer(user.expired)} - ${user.role === "VIP" ? "Unlimited" : user.limit} Limit</p> 
              </div>
            </a>
          `;
          premiumList.appendChild(userItem);
        });
      },
      error: function(error) {
        console.error('Error fetching premium users:', error);
      }
    });
  }

  selectElement.addEventListener('change', toggleSections);
  toggleSections();
  fetchPremiumUsers();
});
</script>
      
